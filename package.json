{
  "type": "module",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@awmottaz/prettier-plugin-void-html": "^1.8.0",
    "@faker-js/faker": "^9.8.0",
    "prettier": "^3.5.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sqlite3"
    ]
  },
  "prettier": {
    "plugins": [
      "@awmottaz/prettier-plugin-void-html"
